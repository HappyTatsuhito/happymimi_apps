<?xml version="1.0" encoding="utf-8"?>
<launch>
  <!-- Param -->
  <rosparam file="$(find happymimi_params)/param/location/demo_3.yaml" command="load" ns="/location_dict" />
  <!-- Sensor -->
  <include file="$(find happymimi_bringup)/launch/sensor.launch" />

  <!-- Map server -->
  <!-- <arg name="map_file" default="$(find happymimi_navigation)/maps/second_demo.yaml"/> -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find happymimi_navigation)/maps/demo_3.yaml" />
  <!-- <node name="map_server" pkg="map_server" type="map_server" args="map_file" /> -->

  <!-- AMCL -->
  <include file="$(find happymimi_navigation)/launch/include/amcl.launch.xml"/>

  <!-- Move base -->
  <include file="$(find happymimi_navigation)/launch/include/move_base.launch.xml"/>

</launch>
